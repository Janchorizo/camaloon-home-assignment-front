{
  "name": "camaloon-home-assignment-front",
  "version": "1.0.0",
  "description": "Home assignment for the job application process at Camaloon.",
  "main": "src/index.js",
  "repository": "git@github.com:Janchorizo/camaloon-home-assignment-front.git",
  "author": "Janchorizo <jancho@usal.es>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "test": "jest",
    "build": "STAGE=prod bash pre_build.sh && webpack --mode production",
    "dev": "STAGE=dev webpack serve --mode development",
    "lint": "eslint \"./src/**/*.js\""
  },
  "jest": {
    "setupFilesAfterEnv": [
      "./setup-tests.js"
    ],
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mocks__/fileMock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    }
  },
  "dependencies": {
    "jest": "^26.6.3",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "@babel/core": "^7.13.10",
    "@babel/preset-env": "^7.13.10",
    "@babel/preset-react": "^7.12.13",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.5.0",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "core.js": "^0.4.2",
    "css-loader": "^5.1.3",
    "enzyme": "^3.11.0",
    "eslint": "^7.22.0",
    "eslint-config-google": "^0.14.0",
    "eslint-plugin-react": "^7.22.0",
    "identity-obj-proxy": "^3.0.0",
    "import": "^0.0.6",
    "regenerator-runtime": "^0.13.7",
    "style-loader": "^2.0.0",
    "webpack": "^5.26.3",
    "webpack-cli": "^4.5.0",
    "webpack-dev-server": "^3.11.2"
  }
}
